{
    "name": "mtownsend/response-xml",
    "description": "The missing XML support for Laravel's Response class.",
    "keywords": [
        "response",
        "laravel",
        "api",
        "xml"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Townsend",
            "email": "mtownsend5512@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mtownsend\\ResponseXml\\": "src"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/http": "^9.0",
        "illuminate/routing":  "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/container": "^9.0",
        "spatie/array-to-xml": "^2.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "laravel/framework": "^9.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Mtownsend\\ResponseXml\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mtownsend\\ResponseXml\\Providers\\ResponseXmlServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
